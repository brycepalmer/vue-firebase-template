{
  "name": "vue-firebase-template",
  "version": "0.9.0",
  "scripts": {
    "dev": "concurrently -c \"black.bgGreen,black.bgYellow,black.bgBlue\" -p \"[{name}]\" \"yarn:vite\" \"yarn:emulators\" \"yarn:functions\"",
    "dev:local": "yarn build:dev && yarn emulators:full",
    "functions": "cd functions && yarn dev",
    "vite": "vite",
    "emulators": "firebase emulators:start --only auth,firestore,functions,storage --import=.firebase --export-on-exit",
    "emulators:full": "firebase emulators:start --import=.firebase --export-on-exit",
    "deploy": "yarn build:prod && firebase deploy",
    "deploy:hosting": "yarn build:prod && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore:rules",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:prod": "vue-tsc --noEmit && vite build",
    "firebase:login": "firebase login",
    "serve": "vite preview"
  },
  "dependencies": {
    "@quasar/extras": "^1.14.1",
    "@vueuse/core": "^8.7.5",
    "@vueuse/head": "^0.7.6",
    "firebase": "^9.8.4",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.7.3",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.1.16",
    "@quasar/vite-plugin": "^1.0.10",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": ">=13.0.2",
    "postcss-rtlcss": "^3.6.3",
    "pug": "^3.0.2",
    "sass": "1.53.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-icons": "^0.14.5",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.12",
    "vite-aliases": "^0.9.2",
    "vite-plugin-pages": "^0.24.2",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-tsc": "^0.38.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache"
  }
}
